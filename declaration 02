#include <stdio.h>
// function declaration
int sum (int var1,int var2); 
int main () {
int a,b;
scanf("%d %d", &a, &b);
// function call
int result = sum (a,b); 
printf("sum of %d and %d is %d", a, b, result);


    return 0;
}

// function definition
int sum(int num1,int num2){

    int val = num1+num2;
    return val;
}
